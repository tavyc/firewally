-A INPUT -i lo -j ACCEPT
-A INPUT -j INPUT_1
-A INPUT -m limit --limit 1/second --limit-burst 2 -j LOG --log-prefix "FW: INPUT DROP: " --log-level notice
-A INPUT -j DROP
-A INPUT_1 -d server1 -j RETURN
-A INPUT_1 -d server2 -j RETURN
